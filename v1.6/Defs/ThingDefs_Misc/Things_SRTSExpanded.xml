<Defs>

  <ThingDef Name="BomberShuttleSkyfallerBase" ParentName="EtherealThingBase" Abstract="True">
    <thingClass>BomberSkyfaller</thingClass>
    <useHitPoints>false</useHitPoints>
    <drawOffscreen>true</drawOffscreen>
    <tickerType>Normal</tickerType>
    <altitudeLayer>Skyfaller</altitudeLayer>
    <drawerType>RealtimeOnly</drawerType>
    <skyfaller />
  </ThingDef>

  <!-- Superpod -->

	<TransportShipDef>
		<defName>SRTSSuperpod</defName>
		<shipThing>SRTSSuperpod</shipThing>
		<arrivingSkyfaller>SRTSSuperpod_Incoming</arrivingSkyfaller>
		<leavingSkyfaller>SRTSSuperpod_Leaving</leavingSkyfaller>
		<worldObject>TravelingSRTS</worldObject>
		<maxLaunchDistance>86</maxLaunchDistance>
		<playerShuttle>true</playerShuttle>
	</TransportShipDef>
	
  <ThingDef ParentName="ShuttleSkyfallerBase">
    <defName>SRTSSuperpod_Incoming</defName>
    <label>Superpod incoming</label>
    <thingClass>SRTS.SRTSIncoming</thingClass>
    <graphicData>
      <texPath>Superpod</texPath>
      <graphicClass>Graphic_Single</graphicClass>
      <shaderType>CutoutFlying</shaderType>
      <drawSize>(2,2)</drawSize>
    </graphicData>
    <skyfaller>
      <movementType>Decelerate</movementType>
      <shadow>Things/Skyfaller/SkyfallerShadowDropPod</shadow>
      <shadowSize>(2,2)</shadowSize>
      <anticipationSound>DropPod_Fall</anticipationSound>
      <anticipationSoundTicks>100</anticipationSoundTicks>
      <motesPerCell>0</motesPerCell>
			<rotationCurve>
				<points>
					<li>(0,30)</li>
					<li>(0.5,5)</li>
					<li>(0.9,-5)</li>
					<li>(0.95,0)</li>
				</points>
			</rotationCurve>
			<zPositionCurve>
				<points>
					<li>(0.95,2.5)</li>
					<li>(1,0)</li>
				</points>
			</zPositionCurve>
			<speedCurve>
				<points>
					<li>(0.6,0.6)</li>
					<li>(0.95,0.1)</li>
				</points>
			</speedCurve>
    </skyfaller>
  </ThingDef>

  <ThingDef ParentName="ShuttleSkyfallerBase">
    <defName>SRTSSuperpod_Leaving</defName>
    <label>Superpod leaving</label>
    <thingClass>SRTS.SRTSLeaving</thingClass>
    <graphicData>
      <texPath>Superpod</texPath>
      <graphicClass>Graphic_Single</graphicClass>
      <shaderType>CutoutFlying</shaderType>
      <drawSize>(2,2)</drawSize>
    </graphicData>
    <skyfaller>
      <movementType>Accelerate</movementType>
      <reversed>true</reversed>
      <shadow>Things/Skyfaller/SkyfallerShadowDropPod</shadow>
      <shadowSize>(2,2)</shadowSize>
      <anticipationSound>Sound_Launch</anticipationSound>
      <anticipationSoundTicks>-10</anticipationSoundTicks>
      <ticksToImpactRange><min>-40</min><max>-15</max></ticksToImpactRange>
			<rotationCurve>
				<points>
					<li>(0,0)</li>
					<li>(0.15,10)</li>
					<li>(0.5,-5)</li>
				</points>
			</rotationCurve>
			<zPositionCurve>
				<points>
					<li>(0,0)</li>
					<li>(0.08,2)</li>
				</points>
			</zPositionCurve>
			<speedCurve>
				<points>
					<li>(0,0.2)</li>
					<li>(0.4,0.7)</li>
				</points>
			</speedCurve>
    </skyfaller>
  </ThingDef>

  <ThingDef>
    <defName>SRTSSuperpod_Active</defName>
    <label>Superpod active</label>
    <category>Item</category>
    <thingClass>ActiveTransporter</thingClass>
    <scatterableOnMapGen>false</scatterableOnMapGen>
    <tickerType>Normal</tickerType>
    <graphicData>
      <texPath>Superpod</texPath>
      <graphicClass>Graphic_Single</graphicClass>
      <drawSize>(2,2)</drawSize>
    </graphicData>
    <altitudeLayer>Building</altitudeLayer>
    <blockWind>true</blockWind>
    <useHitPoints>true</useHitPoints>
    <statBases>
      <MaxHitPoints>150</MaxHitPoints>
      <Flammability>0</Flammability>
    </statBases>
    <selectable>true</selectable>
  </ThingDef>


